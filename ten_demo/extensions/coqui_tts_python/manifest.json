{
    "type": "extension",
    "name": "coqui_tts_python",
    "version": "0.1.0",
    "dependencies": [
        {
            "type": "system",
            "name": "ten_runtime_python",
            "version": "0.11"
        },
        {
            "type": "system",
            "name": "ten_ai_base",
            "version": "=0.7.22"
        }
    ],
    "api": {
        "interface": [
            {
                "import_uri": "../../system/ten_ai_base/api/tts2-interface.json"
            }
        ],
        "property": {
            "properties": {
                "model_name": {
                    "type": "string",
                    "default": "tts_models/en/ljspeech/tacotron2-DDC"
                },
                "vocoder_name": {
                    "type": "string"
                },
                "language": {
                    "type": "string",
                    "default": "en"
                },
                "sample_rate": {
                    "type": "integer",
                    "default": 22050
                },
                "device": {
                    "type": "string",
                    "default": "cpu"
                }
            }
        }
    },
    "package": {
        "include": [
            "manifest.json",
            "property.json",
            "**.py",
            "requirements.txt"
        ]
    }
}
