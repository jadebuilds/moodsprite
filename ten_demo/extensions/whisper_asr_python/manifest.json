{
    "type": "extension",
    "name": "whisper_asr_python",
    "version": "0.1.0",
    "dependencies": [
        {
            "type": "system",
            "name": "ten_runtime_python",
            "version": "0.11"
        },
        {
            "type": "system",
            "name": "ten_ai_base",
            "version": "=0.7.22"
        }
    ],
    "api": {
        "interface": [
            {
                "import_uri": "../../system/ten_ai_base/api/asr-interface.json"
            }
        ],
        "property": {
            "properties": {
                "model": {
                    "type": "string",
                    "default": "base"
                },
                "language": {
                    "type": "string",
                    "default": "en"
                },
                "sample_rate": {
                    "type": "integer",
                    "default": 16000
                },
                "device": {
                    "type": "string",
                    "default": "cpu"
                },
                "task": {
                    "type": "string",
                    "default": "transcribe"
                },
                "chunk_length_s": {
                    "type": "integer",
                    "default": 30
                }
            }
        }
    },
    "package": {
        "include": [
            "manifest.json",
            "property.json",
            "**.py",
            "requirements.txt"
        ]
    }
}
